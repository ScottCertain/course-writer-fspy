[run]
source = .
omit = 
    tests/*
    */site-packages/*
    */dist-packages/*
    */virtualenv/*
    */venv/*
    */env/*
    */tmp/*
    setup.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError
    def __str__

[html]
directory = test_logs/htmlcov
